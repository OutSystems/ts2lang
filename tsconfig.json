{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "bin"
    },
    "filesGlob": [
        "**/*.ts",
        "**/*.tsx",
        "!node_modules/**"
    ],
    "files": [
        "src/ts-analyser.ts",
        "src/ts2lang.ts",
        "src/Types/ITsType.ts",
        "src/Types/TsAnyType.ts",
        "src/Types/TsArrayType.ts",
        "src/Types/TsBasicType.ts",
        "src/Types/TsFunctionType.ts",
        "src/Types/TsIdentifierType.ts",
        "src/Units/AbstractTsUnit.ts",
        "src/Units/ITsUnit.ts",
        "src/Units/TsAnnotation.ts",
        "src/Units/TsClass.ts",
        "src/Units/TsEnumerate.ts",
        "src/Units/TsFunction.ts",
        "src/Units/TsImport.ts",
        "src/Units/TsInterface.ts",
        "src/Units/TsModule.ts",
        "src/Units/TsVariable.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
